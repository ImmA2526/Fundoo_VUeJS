ate v-slot=\"{ errors }\">   \r\n    \r\n      <div class=\"form-group row\">\r\n        <div class=\"col-sm-7\">\r\n          <input v-model=\"email\" type=\"email\" id=\"email\" name=\"EmailID\" required />\r\n          <span id=\"em\">Email</span>\r\n           <span>{{ errors[0] }}</span>\r\n        </div>\r\n      </div>\r\n      </ValidationProvider>\r\n      <div class=\"form-group row\">\r\n        <div class=\"col-sm-7\">\r\n          <input type=\"password\" id=\"pass\" name=\"Password\" required />\r\n          <span>Password</span>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"form-group row\">\r\n        <div class=\"forgot\"><a href=\"Forgot_PWD.html\">Forgot Password</a></div>\r\n      </div>\r\n      <div class=\"form-group row\">\r\n        <div class=\"col-sm-1\"></div>\r\n        <div class=\"col-sm-10\">\r\n          <a id=\"ok\" href=\"Register.html\">Create</a>\r\n          <button id=\"bt\" type=\"submit\" class=\"btn btn-primary\">Login</button>\r\n        </div>\r\n      </div>\r\n      <div class=\"blank\"></div>\r\n    </form>\r\n    </ValidationObserver>\r\n\r\n  </div>\r\n</template>\r\n\r\n<style>\r\n.h2 {\r\n  padding-bottom: 20px;\r\n  margin-top: 1px;\r\n  font-family: Cambria, Cochin, Georgia, Times, \"Times New Roman\", serif;\r\n  color: rgb(252, 248, 6);\r\n  padding-bottom: 10px;\r\n  background-color: black;\r\n  border-radius: 14px;\r\n}\r\n\r\n#h2 {\r\n  color: white;\r\n}\r\n\r\n.blank {\r\n  padding-bottom: 6px;\r\n  background-color: black;\r\n  border-radius: 20px;\r\n}\r\n/* form */\r\n.logform {\r\n  /* display: grid;\r\n    justify-content: center; */\r\n  border: 2px solid #f1f1f1;\r\n  border-radius: 12px;\r\n  width: 60%;\r\n  height: 64%;\r\n  margin-top: 160px;\r\n}\r\n\r\n/* Center form */\r\n.full {\r\n  margin-left: 600px;\r\n}\r\n\r\n/*Forgot Link*/\r\n.forgot {\r\n  padding-left: 140px;\r\n  margin-top: 6px;\r\n  margin-bottom: 12px;\r\n}\r\n\r\n/* Input Box */\r\ninput[type=\"text\"],\r\ninput[type=\"password\"],\r\ninput[type=\"email\"],\r\ntextarea,\r\nselect {\r\n  padding: 14px 20px;\r\n  margin: 11px 0;\r\n  width: 100%;\r\n  height: 30px;\r\n  margin-left: 40%;\r\n  border-color: deepskyblue;\r\n  position: relative;\r\n  border-radius: 10px;\r\n}\r\n\r\n/* SPAN AND BOX UPR  */\r\n\r\n#em {\r\n  width: 40px;\r\n}\r\nspan {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50px;\r\n  transform: translateY(-50%);\r\n  transition: 0.5;\r\n  margin-inline: 120px;\r\n  pointer-events: none;\r\n  width: 80px;\r\n}\r\n\r\ninput:focus + span,\r\ninput:valid + span {\r\n  top: 0;\r\n  left: 40px;\r\n  color: dodgerblue;\r\n  padding: 2px;\r\n  background: white;\r\n}\r\n\r\n/* Butoon */\r\n#bt {\r\n  background-color: rgb(46, 135, 250);\r\n  border-radius: 20px;\r\n  margin-bottom: 10px;\r\n  cursor: pointer;\r\n  width: 24%;\r\n  margin-left: 220px;\r\n  height: 40px;\r\n}\r\n\r\n#bt:hover {\r\n  background-color: rgba(76, 117, 252, 0.815);\r\n}\r\n\r\n/* create button */\r\n#ok {\r\n  /* margin-right: 20px; */\r\n  padding-left: 80px;\r\n}\r\n\r\n.a {\r\n  text-decoration: none;\r\n  padding-left: 20%;\r\n}\r\n</style>\r\n \r\n\r\n <script>\r\nexport default {\r\n  data: () => ({\r\nemail: '',\r\n    \r\n    password: ''\r\n  }),\r\n  methods: {\r\n    onSubmit () {\r\n      alert('Form has been submitted!');\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}