,OAAA,EAAA;AANA,KAAA;AAAA,GAHA;AAWA,EAAA,WAAA,EAAA;AACA,IAAA,IAAA,EAAA;AACA,MAAA,KAAA,EAAA;AACA,QAAA,QAAA,EAAA,QADA;AAEA,QAAA,KAAA,EAAA;AAFA,OADA;AAKA,MAAA,QAAA,EAAA;AACA,QAAA,QAAA,EAAA,QADA;AAEA,QAAA,SAAA,EAAA,SAAA,CAAA,CAAA;AAFA;AALA;AADA,GAXA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,kBADA,8BACA,SADA,EACA;AACA,UAAA,KAAA,GAAA,KAAA,EAAA,CAAA,IAAA,CAAA,SAAA,CAAA;;AAEA,UAAA,KAAA,EAAA;AACA,eAAA;AACA,wBAAA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA;AADA,SAAA;AAGA;AACA,KATA;AAWA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,6DAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KADA;AAEA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QAFA;AAGA,QAAA,MAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,OARA;AASA,KAtBA;AAwBA,IAAA,SAxBA,uBAwBA;AACA,WAAA,EAAA,CAAA,MAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,IAAA;AACA,KA5BA;AA6BA,IAAA,QA7BA,sBA6BA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA,CADA,CAGA;;AACA,MAAA,MAAA,CAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,SAAA,aAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,SAAA;AACA,OALA,EAKA,IALA;AAMA,KAvCA;AAwCA,IAAA,YAxCA,0BAwCA;AACA,WAAA,EAAA,CAAA,MAAA;;AAEA,UAAA,CAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA,aAAA,QAAA;AACA;AACA;AA9CA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <form novalidate class=\"md-layout\" @submit.prevent=\"validateUser\">\r\n      <md-card class=\"md-layout-item md-size-50 md-small-size-100\">\r\n        <center> <div class=\"h2\">\r\n                <h2 id=\"h2\">Fundoo Notes</h2>\r\n                <h3>Login</h3>\r\n            </div></center>\r\n            \r\n\r\n        <md-card-content>\r\n          <md-field :class=\"getValidationClass('email')\">\r\n            <label for=\"email\">Email</label>\r\n            <md-input type=\"email\" name=\"email\" id=\"email\" autocomplete=\"email\" v-model=\"form.email\" :disabled=\"sending\" />\r\n            <span class=\"md-error\" v-if=\"!$v.form.email.required\">The email is required</span>\r\n            <span class=\"md-error\" v-else-if=\"!$v.form.email.email\">Invalid email</span>\r\n          </md-field>\r\n          <md-field :class=\"getValidationClass('password')\">\r\n            <label for=\"password\">Password</label>\r\n            <md-input type=\"password\" name=\"password\" id=\"password\" autocomplete=\"password\" v-model=\"form.password\" :disabled=\"sending\" />\r\n            <span class=\"md-error\" v-if=\"!$v.form.password.required\">The password is required</span>\r\n            <span class=\"md-error\" v-else-if=\"!$v.form.password.minlength\">Invalid password</span>\r\n          </md-field>\r\n             \r\n             <md-button to=\"./Forgot\" class=\"md-primary\">Forgot Password?</md-button>\r\n              \r\n                    </md-card-content>\r\n\r\n        <md-progress-bar md-mode=\"indeterminate\" v-if=\"sending\" />\r\n\r\n        <md-card-actions>\r\n          <div class=\"md-layout md-gutter\">\r\n            <div class=\"md-layout-item md-small-size-100\">\r\n              <md-button to=\"./Register\" class=\"md-primary\" :disabled=\"sending\">Create Account</md-button>\r\n              <!-- <md-button  type=\"submit\" class=\"md-primary\" >Create account</md-button> -->\r\n            </div>\r\n            <div class=\"md-layout-item md-small-size-100\">\r\n              <md-button v-on:click=\"post\" type=\"submit\" class=\"md-dense md-raised md-primary\" :disabled=\"sending\" >Login</md-button>\r\n            </div>\r\n          </div>\r\n        </md-card-actions>\r\n        <div class=\"blank\"></div>\r\n      </md-card>\r\n<md-snackbar :md-active.sync=\"userSaved\">The user {{ loginUser }} successfully login!</md-snackbar>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { validationMixin } from \"vuelidate\";\r\nimport { required, email, minLength } from \"vuelidate/lib/validators\";\r\n\r\nexport default {\r\n  name: \"FormValidation\",\r\n  mixins: [validationMixin],\r\n  data: () => ({\r\n    form: {\r\n      email: null,\r\n      password: null\r\n    },\r\n    userSaved: false,\r\n    sending: false\r\n  }),\r\n  validations: {\r\n    form: {\r\n      email: {\r\n        required,\r\n        email\r\n      },\r\n      password: {\r\n        required,\r\n        minLength: minLength(8)\r\n      }\r\n    }\r\n  },\r\n\r\n\r\n  methods: {\r\n    getValidationClass(fieldName) {\r\n      const field = this.$v.form[fieldName];\r\n\r\n      if (field) {\r\n        return {\r\n          \"md-invalid\": field.$invalid && field.$dirty\r\n        };\r\n      }\r\n    },\r\n\r\n// loginUser\r\n    post:function(){\r\n      this.$http.post('http://fundoonotes.incubation.bridgelabz.com/api/user/login',{\r\n        email:this.form.email,\r\n        password:this.form.password,\r\n        cartId:''\r\n\r\n      }).then(function(data){\r\n        this.$router.push(\"/reset\")\r\n        console.log(data);\r\n      });\r\n    },\r\n\r\n    clearForm() {\r\n      this.$v.$reset();\r\n      this.form.email = null;\r\n      this.form.password = null;\r\n    },\r\n    saveUser() {\r\n      this.sending = true;\r\n\r\n      // Instead of this timeout, here you can call your API\r\n      window.setTimeout(() => {\r\n        this.loginUser = `${this.form.email} `;\r\n        this.userSaved = true;\r\n        this.sending = false;\r\n        this.clearForm();\r\n      }, 1500);\r\n    },\r\n    validateUser() {\r\n      this.$v.$touch();\r\n\r\n      if (!this.$v.$invalid) {\r\n        this.saveUser();\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n.h2 {\r\n    padding-bottom: 10px;\r\n    margin-top: 1px;\r\n    padding-top: 6px;\r\n    font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;\r\n    color:rgb(252, 248, 6);\r\n    padding-bottom: 10px;\r\n    background-color:black;\r\n    border-radius: 14px;\r\n} \r\n\r\n#h2\r\n{\r\n  color:white;\r\n}\r\n\r\n.blank\r\n{\r\n    padding-bottom: 6px;\r\n    background-color:black;\r\n    border-radius: 20px;\r\n\r\n}\r\n.md-progress-bar {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  left: 0;\r\n}\r\n.md-card {\r\n  margin-left: 400px;\r\n  margin-top: 150px;\r\n}\r\n</style>"],"sourceRoot":"src/Pages"}]}