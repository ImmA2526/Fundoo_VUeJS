A,QAAA,IAAA,KAAA,CAAA;AADA,SAAA;AAGA;AACA,KATA;AAUA;AAEA,IAAA,GAAA,EAAA,eAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,6DAAA,EAAA;AACA,QAAA,OAAA,EAAA,KAAA,IAAA,CAAA,QADA;AAEA,QAAA,SAAA,EAAA,KAAA,IAAA,CAAA,SAFA;AAGA,QAAA,MAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,OARA;AASA,KAtBA;AAwBA,IAAA,SAxBA,uBAwBA;AACA,WAAA,EAAA,CAAA,MAAA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,SAAA,GAAA,IAAA;AACA,KA5BA;AA6BA,IAAA,QA7BA,sBA6BA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA,CADA,CAGA;;AACA,MAAA,MAAA,CAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,SAAA,aAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,SAAA;AACA,OALA,EAKA,IALA;AAMA,KAvCA;AAwCA,IAAA,YAxCA,0BAwCA;AACA,WAAA,EAAA,CAAA,MAAA;;AAEA,UAAA,CAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA,aAAA,QAAA;AACA;AACA;AA9CA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- <form > -->\r\n    <form novalidate class=\"md-layout\" @submit.prevent=\"validateUser\">\r\n   \r\n      <md-card class=\"md-layout-item md-size-50 md-small-size-100\">\r\n        <center>\r\n          <div class=\"h2\">\r\n            <h2 id=\"h2\">Fundoo Notes</h2>\r\n            <h3>Reset Password</h3>\r\n          </div>\r\n        </center>\r\n\r\n        <md-card-content>\r\n          <md-field :class=\"getValidationClass('password')\">\r\n            <label for=\"password\">Old Password</label>\r\n            <md-input type=\"password\" name=\"password\" id=\"password\" autocomplete=\"password\" v-model=\"form.password\" :disabled=\"sending\" />\r\n            <span class=\"md-error\" v-if=\"!$v.form.password.required\" >The password is required</span>\r\n            <span class=\"md-error\" v-else-if=\"!$v.form.password.minlength\" >Invalid password</span>\r\n          </md-field>\r\n<!-- New password for reseting -->\r\n          <md-field :class=\"getValidationClass('password')\">\r\n            <label for=\"password\">New Password</label>\r\n            <md-input type=\"password\" name=\"newPassword\" id=\"password\" v-model=\"password\" :disabled=\"sending\" />\r\n            <span class=\"md-error\" v-if=\"!$v.form.cpassword.required\" >The password is required</span>\r\n            <span class=\"md-error\" v-else-if=\"!$v.form.cpassword.minlength\" >Invalid password</span>\r\n          </md-field>\r\n\r\n          <md-field :class=\"getValidationClass('password')\">\r\n            <label for=\"Newpassword\">Confirm Password</label>\r\n            <md-input type=\"password\" name=\"Confirmpassword\" id=\"cpass\" v-model=\"cpassword\"  :disabled=\"sending\" />\r\n            <span class=\"md-error\" v-if=\"!$v.form.cpassword.required\" >The password is required</span>\r\n            <span class=\"md-error\" v-else-if=\"!$v.form.cpassword.minlength\" >Invalid password</span>\r\n          </md-field>\r\n\r\n        </md-card-content>\r\n\r\n        <md-progress-bar md-mode=\"indeterminate\" v-if=\"sending\" />\r\n\r\n        <md-card-actions>\r\n          <md-button v-on:click=\"put\" type=\"submit\" class=\"md-dense md-raised md-primary\" :disabled=\"sending\">Reset</md-button>\r\n        </md-card-actions>\r\n        <div class=\"blank\"></div>\r\n      </md-card>\r\n      <md-snackbar :md-active.sync=\"userSaved\">The user {{ loginUser }} successfully login!</md-snackbar>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import axios from 'axios'\r\n\r\nimport { validationMixin } from \"vuelidate\";\r\nimport { required, minLength } from \"vuelidate/lib/validators\";\r\n\r\nexport default {\r\n  name: \"FormValidation\",\r\n  mixins: [validationMixin],\r\n  data: () => ({\r\n    form: {\r\n      password: null,\r\n       cpassword:null,\r\n    },\r\n    userSaved: false,\r\n    sending: false,\r\n  }),\r\n\r\n  validations: {\r\n    form: {\r\n      password: {\r\n        required,\r\n        minLength: minLength(8),\r\n      },\r\n// resetpwd\r\n       cpassword:{\r\n         required,\r\n         minLength:minLength(8),\r\n       },\r\n    },\r\n  },\r\n  methods: {\r\n    getValidationClass(fieldName) {\r\n      const field = this.$v.form[fieldName];\r\n\r\n      if (field) {\r\n        return {\r\n          \"md-invalid\": field.$invalid && field.$dirty,\r\n        };\r\n      }\r\n    },\r\n//reset \r\n\r\n put:function(){\r\n      this.$http.put('http://fundoonotes.incubation.bridgelabz.com/api/user/reset',{\r\n        passwor:this.form.password,\r\n        cpassword:this.form.cpassword,\r\n        cartId:'',\r\n\r\n      }).then(function(data){\r\n        this.$router.push(\"/login\")\r\n        console.log(data);\r\n      });\r\n    },\r\n\r\n    clearForm() {\r\n      this.$v.$reset();\r\n      this.form.password = null;\r\n      this.form.cpassword=null;\r\n    },\r\n    saveUser() {\r\n      this.sending = true;\r\n\r\n      // Instead of this timeout, here you can call your API\r\n      window.setTimeout(() => {\r\n        this.loginUser = `${this.form.email} `;\r\n        this.userSaved = true;\r\n        this.sending = false;\r\n        this.clearForm();\r\n      }, 1500);\r\n    },\r\n    validateUser() {\r\n      this.$v.$touch();\r\n\r\n      if (!this.$v.$invalid) {\r\n        this.saveUser();\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.h2 {\r\n  padding-bottom: 10px;\r\n  padding-top: 6px;\r\n  // margin-top: 1px;\r\n  font-family: Cambria, Cochin, Georgia, Times, \"Times New Roman\", serif;\r\n  color: burlywood;\r\n\r\n  padding-bottom: 10px;\r\n  background-color: rgba(78, 52, 12, 0.966);\r\n  border-radius: 14px;\r\n}\r\n\r\n#h2 {\r\n  color: rgb(229, 245, 13);\r\n}\r\n.blank {\r\n  padding-bottom: 6px;\r\n  background-color: rgba(78, 52, 12, 0.966);\r\n  border-radius: 20px;\r\n}\r\n\r\n.md-progress-bar {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  left: 0;\r\n}\r\n.md-card {\r\n  margin-left: 400px;\r\n  margin-top: 150px;\r\n}\r\n</style>"]