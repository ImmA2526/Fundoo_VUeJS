r\n  },\r\n  methods: {\r\n    onSubmit() {\r\n      this.$validator.validateAll().then(result => {\r\n        if (result) {\r\n          alert(\"submit\");\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>"]}]}